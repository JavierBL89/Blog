<%- include('Partials/header'); -%>


<form class="" action="/composse" method="post">
  <div class="form-group">
    <label for="inputTitle">Post Title</label>
    <input class="form-control" type="text" id="inputTitle" name="newPostTitle">
  </div>
  <div class="form-group">
    <label for="inputContent">Post Content</label>
    <textarea class="form-control" rows="8" cols="80"  id="inputContent" name="newPostContent"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="submit">Post</button>
</form>


